<p id="notice"><%= notice %></p>

<h1>Trip Schedules</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trip_schedules.each do |trip_schedule| %>
      <tr>
        <td><%= trip_schedule.user %></td>
        <td><%= link_to 'Show', trip_schedule %></td>
        <td><%= link_to 'Edit', edit_trip_schedule_path(trip_schedule) %></td>
        <td><%= link_to 'Destroy', trip_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trip Schedule', new_trip_schedule_path %>
