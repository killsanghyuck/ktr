<p id="notice"><%= notice %></p>

<h1>Trip Schedule Details</h1>

<table>
  <thead>
    <tr>
      <th>Trip schudule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trip_schedule_details.each do |trip_schedule_detail| %>
      <tr>
        <td><%= trip_schedule_detail.trip_schudule %></td>
        <td><%= link_to 'Show', trip_schedule_detail %></td>
        <td><%= link_to 'Edit', edit_trip_schedule_detail_path(trip_schedule_detail) %></td>
        <td><%= link_to 'Destroy', trip_schedule_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

